    "@storybook/addon-essentials": "^6.0.22",
    "@types/ci-info": "latest",
    "@types/inquirer": "latest",
    "babel-eslint": "latest",
    "eslint": "latest",
    "node-sass": "^4.14.1",
    "nuxt-edge": "^2.14.2-26626271.34c56722",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2",
    "standard-version": "latest",
    "typescript": "^4.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
