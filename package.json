{
  "name": "@nuxtjs/storybook",
  "version": "0.0.0",
  "description": "Storybook integration with Nuxt",
  "repository": "nuxt-community/nuxt-storybook",
  "license": "MIT",
  "bin": {
    "nuxt-storybook": "bin/nuxt-storybook.js"
  },
  "files": [
    "bin",
    "dist",
    "storybook"
  ],
  "scripts": {
    "build": "yarn clean && tsc && tsc -p tsconfig.browser.json",
    "clean": "rimraf dist",
    "dev": "yarn nuxt-storybook example dev",
    "nuxt-storybook": "./bin/nuxt-storybook.js",
    "storybook": "yarn nuxt-storybook example",
    "test": "yarn lint",
    "lint": "eslint --ext .ts,.js,.vue .",
    "release": "yarn test && standard-version && yarn build && git push --follow-tags && npm publish"
  },
  "dependencies": {
    "@storybook/vue": "^5.3.19",
    "babel-preset-vue": "^2.0.2",
    "consola": "^2.14.0",
    "jiti": "^0.1.11"
  },
  "devDependencies": {
    "@babel/preset-env": "latest",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@nuxtjs/proxy": "^2.0.0",
    "@types/ci-info": "latest",
    "@types/inquirer": "latest",
    "babel-eslint": "latest",
    "core-js": "2",
    "eslint": "latest",
    "nuxt-edge": "latest",
    "rimraf": "latest",
    "standard-version": "latest",
    "typescript": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
