{
  "name": "@nuxtjs/storybook",
  "version": "0.0.1",
  "description": "Storybook integration with Nuxt",
  "repository": "nuxt-community/nuxt-storybook",
  "license": "MIT",
  "bin": {
    "nuxt-storybook": "bin/nuxt-storybook.js"
  },
  "files": [
    "bin",
    "dist",
    "storybook"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "dev": "yarn nuxt-storybook playground dev",
    "nuxt-storybook": "ts-node src/cli",
    "test": "yarn lint",
    "lint": "eslint --ext .ts,.js,.vue .",
    "release": "yarn test && standard-version && yarn build && git push --follow-tags && npm publish"
  },
  "dependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/vue": "^5.3.19",
    "babel-preset-vue": "^2.0.2",
    "consola": "^2.14.0",
    "jiti": "^0.1.11"
  },
  "devDependencies": {
    "@babel/preset-env": "latest",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/proxy": "^2.0.0",
    "@nuxtjs/tailwindcss": "^2.0.0",
    "@types/ci-info": "latest",
    "@types/inquirer": "latest",
    "babel-eslint": "latest",
    "core-js": "2",
    "eslint": "latest",
    "node-sass": "^4.14.1",
    "nuxt-edge": "latest",
    "rimraf": "latest",
    "sass-loader": "^9.0.2",
    "standard-version": "latest",
    "typescript": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
