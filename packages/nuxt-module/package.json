{
  "name": "@nuxtjs/storybook",
  "version": "9.0.1",
  "description": "Storybook module for Nuxt",
  "license": "MIT",
  "homepage": "https://storybook.nuxtjs.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/storybook.git",
    "directory": "packages/nuxt-module"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/storybook/issues"
  },
  "keywords": [
    "storybook",
    "nuxt",
    "vite",
    "vue3"
  ],
  "author": {
    "name": "ChakAs3",
    "email": "javachakir@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs"
    },
    "./preset": {
      "types": "./dist/preset.d.mts",
      "import": "./dist/preset.mjs"
    },
    "./preview": {
      "types": "./dist/preview.d.mts",
      "import": "./dist/preview.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.mts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ],
      "preset": [
        "./dist/preset.d.mts"
      ],
      "preview": [
        "./dist/preview.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev:prepare": "unbuild --stub",
    "build": "unbuild",
    "prepack": "unbuild"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^3.0.0",
    "@nuxt/kit": "^3.18.1 || ^4.0.0",
    "@storybook-vue/nuxt": "workspace:*",
    "chalk": "^5.6.2",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "get-port-please": "^3.2.0",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "1.0.2",
    "storybook": "10.2.4",
    "unbuild": "3.6.1",
    "vite": "7.3.0"
  },
  "peerDependencies": {
    "storybook": "^10.1.11"
  }
}
