{
  "name": "@storybook-vue/nuxt",
  "version": "9.0.1",
  "description": "Storybook addon for Nuxt and Vite",
  "license": "MIT",
  "homepage": "https://storybook.nuxtjs.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/storybook.git",
    "directory": "packages/storybook-addon"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/storybook/issues"
  },
  "keywords": [
    "storybook",
    "nuxt",
    "vite",
    "vue3"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "default": "./dist/preset.mjs"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    },
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "template/**/*",
    "README.md",
    "*.js",
    "*.mjs",
    "*.cjs",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "dev": "tsdown --watch",
    "prepack": "tsdown",
    "release": "pnpm changelogen --release --push && pnpm publish"
  },
  "peerDependencies": {
    "nuxt": "^4.0.0",
    "storybook": "^10.0.0",
    "vite": "^5.2.0 || ^6.0.0 || ^7.0.0",
    "vue": "^3.5.24"
  },
  "dependencies": {
    "@nuxt/kit": "4.2.1",
    "@nuxt/schema": "4.2.1",
    "@nuxt/vite-builder": "4.2.1",
    "@rollup/plugin-replace": "^6.0.3",
    "@storybook/builder-vite": "10.0.8",
    "@storybook/vue3": "10.0.8",
    "@storybook/vue3-vite": "10.0.8",
    "json-stable-stringify": "^1.3.0",
    "mlly": "^1.8.0",
    "ofetch": "^1.5.1",
    "pathe": "^2.0.3",
    "unctx": "^2.4.1",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "changelogen": "0.6.2",
    "nuxt": "4.2.1",
    "storybook": "10.0.8",
    "tsdown": "^0.16.5",
    "typescript": "5.9.3",
    "vue": "3.5.24"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "managerEntries": [],
    "previewEntries": [
      "./src/index.ts",
      "./src/preview.ts"
    ],
    "nodeEntries": [
      "./src/preset.ts"
    ]
  }
}
